<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.brick4.LoginData">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label>Please sign up
        <padding>
            <Insets bottom="10.0" top="10.0"/>
        </padding>
    </Label>
    <Label fx:id="MessageAreaValue"></Label>
    <Label fx:id="NameAreaLabel"></Label>
    <Label fx:id="EmailAreaLabel"></Label>
    <padding>
        <Insets bottom="5.0" top="5.0"/>
    </padding>
    <Label fx:id="CardAreaLabel"></Label>
    <Label fx:id="BalanceAreaLabel"></Label>
    <padding>
        <Insets bottom="5.0" top="5.0"/>
    </padding>
    <Label fx:id="TransactionsAreaLabel"></Label>

    <Button text="Login" onAction="#loginButton"/>
</VBox>
